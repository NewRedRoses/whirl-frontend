{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.whirl.social/api/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/api/:path*",
      "destination": "https://api.whirl.social/api/:path*",
      "statusCode": 307
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://whirl.social"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,HEAD,PUT,PATCH,POST,DELETE"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type,Authorization"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
